<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.ProfileFragment">

    <LinearLayout
        android:id="@+id/llToolBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_50sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/white"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/profileBackBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10.00dp"
            android:layout_marginTop="19.00dp"
            android:src="@drawable/profile_backbtm"
            tools:ignore="ContentDescription,ImageContrastCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14.00dp"
            android:layout_marginTop="19.00dp"
            android:fontFamily="@font/averia_gruesa_libre"
            android:text="Profile"
            android:textSize="@dimen/_16ssp"
            android:textStyle="bold"
            tools:ignore="HardcodedText,TextContrastCheck" />
    </LinearLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1.50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/llToolBar"
        android:background="@color/black"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/_10sdp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Account"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:fontFamily="@font/averia_gruesa_libre"
            android:textSize="@dimen/_20ssp"
            android:layout_marginStart="15.00dp"
            android:layout_marginTop="10.00dp"
            />

        <TextView
            android:id="@+id/userPhoneNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9999999999"
            android:textColor="@color/black"
            android:fontFamily="@font/averia_gruesa_libre"
            android:textSize="@dimen/_16ssp"
            android:layout_marginStart="15.00dp"
            android:layout_marginTop="10.00dp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="YOUR INFORMATION"
            android:fontFamily="@font/averia_gruesa_libre"
            android:textColor="#6b6c74"
            android:layout_marginTop="19.50dp"
            android:layout_marginStart="14.00dp"
            android:textSize="@dimen/_16ssp"
            />


        <LinearLayout
            android:id="@+id/ordersLl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="18.00dp"
            android:weightSum="1"
            >
            <ImageView
                android:layout_width="30.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/shopping"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your Orders"
                android:textStyle="bold"
                android:layout_weight="0.9"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="5.00dp"
                android:fontFamily="@font/averia_gruesa_libre"
                android:textSize="@dimen/_15ssp"
                />
            <ImageView
                android:id="@+id/ordersBtn"
                android:layout_width="20.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="20.00dp"
                android:src="@drawable/right_arrow"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/addressLl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="18.00dp"
            android:weightSum="1"
            >
            <ImageView
                android:layout_width="30.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/contact_book"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your Address"
                android:textStyle="bold"
                android:layout_weight="0.9"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="5.00dp"
                android:fontFamily="@font/averia_gruesa_libre"
                android:textSize="@dimen/_15ssp"
                />
            <ImageView
                android:id="@+id/addressBtn"
                android:layout_width="20.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="20.00dp"
                android:src="@drawable/right_arrow"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/logOutLl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="18.00dp"
            android:weightSum="1"
            >
            <ImageView
                android:layout_width="30.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/logout"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Log Out"
                android:textStyle="bold"
                android:layout_weight="0.9"
                android:textColor="@color/black"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="5.00dp"
                android:fontFamily="@font/averia_gruesa_libre"
                android:textSize="@dimen/_15ssp"
                />
            <ImageView
                android:id="@+id/logOutBtn"
                android:layout_width="20.00dp"
                android:layout_height="26.00dp"
                android:layout_marginStart="20.00dp"
                android:src="@drawable/right_arrow"
                android:layout_marginTop="4.00dp"
                android:background="@android:color/transparent"
                />

        </LinearLayout>
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>